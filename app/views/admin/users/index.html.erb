<div class="page-header">
  <h1><%= t "admin.user.list_users" %></h1>
</div>
<div class="row">
  <div class="col-md-6 col-md-offset-3">
    <div id="search_form" class="row">
      <%= search_form_for [:admin, @search], class: "form-inline" do |f| %>
        <div class="form-group">
          <%= f.label :username_cont, t("admin.user.username") %>
          <%= f.text_field :username_cont, class: "form-control" %><br>
        </div>
        <%= f.submit t("admin.user.search"), class: "btn btn-info" %>
      <% end %>
    </div>
  </div>
</div>
<div>
  <%= link_to t(:create_user), new_admin_user_path, class: "btn btn-success" %>
</div>
<div id="list-users">
  <%= render "list_users" %>
</div>
